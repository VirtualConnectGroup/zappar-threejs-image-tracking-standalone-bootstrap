{
  "name": "@zappar/sharing",
  "version": "1.1.3",
  "description": "This module allows you to easily save & share snapshots or videos of a WebGL canvas.",
  "main": "out/index.js",
  "scripts": {
    "build-standalone": "node_modules/.bin/webpack --config=webpack.config.standalone.js --mode=production",
    "build-standalone-debug": "node_modules/.bin/webpack --config=webpack.config.dev.js --mode=development",
    "build-module": "node_modules/.bin/tsc",
    "tests": "node_modules/.bin/webpack --config=webpack.config.tests.js --mode=development && node_modules/.bin/webpack-dev-server --config=webpack.config.tests.js",
    "update-readme-code": "md-magic --path README.md"
  },
  "keywords": [
    "webgl",
    "screenshot",
    "canvas",
    "zappar",
    "UAR",
    "AR",
    "WebAR",
    "screencap",
    "camera",
    "webshare",
    "navigator.share"
  ],
  "author": "Zappar Limited",
  "license": "Proprietary",
  "files": [
    "README.md",
    "out/**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/dat.gui": "^0.7.7",
    "@types/three": "^0.132.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@zappar/video-recorder": "^0.1.22",
    "@zappar/zappar-threejs": "^0.3.33",
    "dat.gui": "^0.7.7",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "html-webpack-plugin": "^4.5.0",
    "markdown-magic-block": "0.0.4",
    "three": "^0.131.0",
    "ts-loader": "^8.0.12",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "csstype": "^3.0.5",
    "ua-parser-js": "^0.7.28"
  }
}
